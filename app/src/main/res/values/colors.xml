      <color name="yourGreenColor">#3a3434</color>
      <color name="gray">#3a3434</color>
        <color name="colorPrimary">#6200EE</color>
    <color name="colorPrimaryDark">#3700B3</color>
  
    <color name="colorPrimaryText">#FFFFFF</color> <!-- White text color for selected tab -->
